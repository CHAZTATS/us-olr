          "configurations": {
            "development-whirlpool": {
              "buildTarget": "us-olr:build:development-whirlpool"
            },
            "test-whirlpool": {
              "buildTarget": "us-olr:build:test-whirlpool"
            },
            "staging-whirlpool": {
