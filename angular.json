                "src/themes/whirlpool.scss"
              ],
              "assets": [
                {
                  "glob": "*",
                  "input": "public/assets"
                },
                {
                  "glob": "**/*",
                  "input": "public/assets/whirlpool"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "config/brand/brand.ts",
                  "with": "config/brand/whirlpool.ts"
                }
              ]
            },
